{
  "collectionName": "components_section_json_structure_for_google_bot_json_ld_schemas",
  "info": {
    "displayName": "JSON LD Schema",
    "description": ""
  },
  "options": {},
  "attributes": {
    "schemaType": {
      "type": "enumeration",
      "enum": [
        "WebPage",
        "Article",
        "BlogPosting",
        "TechArticle",
        "SoftwareApplication",
        "Product",
        "FAQPage",
        "BreadcrumbList",
        "Event",
        "Organization"
      ],
      "required": true,
      "default": "WebPage",
      "description": "Primary schema.org @type for the page. Used to determine which JSON-LD properties are rendered."
    },
    "headline": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "url": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "type": {
      "type": "string"
    },
    "publishedDate": {
      "type": "date"
    },
    "authors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::author.author"
    },
    "proficiencyLevel": {
      "type": "string"
    },
    "audienceType": {
      "type": "string"
    },
    "about": {
      "type": "string"
    },
    "faqs": {
      "type": "component",
      "repeatable": true,
      "component": "elements.faq-item"
    }
  }
}
