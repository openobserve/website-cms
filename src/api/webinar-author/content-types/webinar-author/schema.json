{
  "kind": "collectionType",
  "collectionName": "webinar_authors",
  "info": {
    "singularName": "webinar-author",
    "pluralName": "webinar-authors",
    "displayName": "Webinar Author",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "bio": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "youtubeUrl": {
      "type": "string"
    },
    "twitterUrl": {
      "type": "string"
    },
    "facebookUrl": {
      "type": "string"
    },
    "instagramUrl": {
      "type": "string"
    },
    "webinars": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::webinar-post.webinar-post",
      "mappedBy": "authors"
    }
  }
}
