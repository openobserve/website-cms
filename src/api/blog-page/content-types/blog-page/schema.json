{
  "kind": "collectionType",
  "collectionName": "blog_pages",
  "info": {
    "singularName": "blog-page",
    "pluralName": "blog-pages",
    "displayName": "Blog Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "publishDate": {
      "type": "date",
      "required": true
    },
    "customerStories": {
      "type": "boolean",
      "default": false
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "blogs"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "blogs"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "blog_pages"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo"
    },
    "isUpdatePublishDate": {
      "type": "boolean"
    },
    "structuredSchema": {
      "type": "component",
      "repeatable": false,
      "component": "section-json-structure-for-google-bot.json-ld-schema"
    },
    "keyOutcomes": {
      "type": "component",
      "repeatable": true,
      "component": "elements.feature-description"
    },
    "faqs": {
      "type": "component",
      "repeatable": false,
      "component": "section-faqs.frequently-asked-question"
    }
  }
}
